        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- map -->
    <definition name="map-layout" template="/WEB-INF/views/layout/layout-map.jsp">
        <put-attribute name="body" value="" />
